{
  "name": "@hebilicious/reproduire",
  "version": "0.0.6",
  "license": "MIT",
  "repository": "Hebilicious/reproduire",
  "private": true,
  "packageManager": "pnpm@8.6.11",
  "main": "./dist/index.cjs",
  "files": [
    "dist",
    "*.cjs"
  ],
  "scripts": {
    "build": "unbuild && ncc build -o action",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "release": "pnpm lint:fix && pnpm build && changelogen --release --push"
  },
  "devDependencies": {
    "@hebilicious/eslint-config": "0.0.3-beta.1",
    "@types/node": "^20.4.5",
    "@vercel/ncc": "^0.36.1",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0-rc.0"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
